ZIM_NOTEBOOK=$1
ZIM_PAGE="Public"
ZIM_EXPORT_DIR=/home/iros/zim-export
ZIM_OUT_PAGE=$ZIM_EXPORT_DIR/$ZIM_PAGE/index.html

echo $0: exporting "$ZIM_NOTEBOOK":"$ZIM_PAGE"... >> /home/iros/out.txt

zim --export --recursive --output=$ZIM_OUT_PAGE --overwrite --format=html --template=Default "$ZIM_NOTEBOOK" "$ZIM_PAGE" >> /home/iros/out.txt

echo $0: exporting $ZIM_NOTEBOOK:$ZIM_PAGE...done >> /home/iros/out.txt

